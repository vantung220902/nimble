{
  "name": "nimble-code-challenge",
  "description": "nimble-code-challenge",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build-app": "tsc && vite build",
    "preview": "vite preview",
    "prepare": "husky install",
    "fix": "pnpm lint --fix",
    "build-sw": "workbox generateSW workbox-config.cjs",
    "build": "pnpm run build-app && pnpm run build-sw",
    "detect-secrets": "git diff --staged --name-only | xargs detect-secrets-hook --baseline .secrets.baseline",
    "fix-detect-secret": "chmod ug+x .husky/* && chmod ug+x bin/* && detect-secrets scan --exclude-files buildspec.yml --exclude-files pnpm-lock.yaml > .secrets.baseline && detect-secrets audit .secrets.baseline",
    "audit": "npx --yes audit-ci -l",
    "init-fe": "bash ./bash/init-fe.sh"
  },
  "lint-staged": {
    "*.{js,ts,tsx,jsx,scss}": [
      "prettier --write"
    ],
    "*.{js,ts,tsx,jsx}": [
      "eslint --fix"
    ]
  },
